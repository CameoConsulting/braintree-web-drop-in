{
  "extends": [
    "braintree/client",
    "braintree/es6"
  ],
  "parserOptions": {
    "sourceType": "script",
    "ecmaVersion": 2018
  },
  "env": {
    "es6": true,
    "browser": true,
  },
  "rules": {
    "object-curly-spacing": [
      "error",
      "always",
      {
        "arraysInObjects": false,
        "objectsInObjects": false
      }
    ]
  }
}
